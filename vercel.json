{
  "redirects": [
    {
      "source": "/robots.txt",
      "destination": "/robots.txt",
      "statusCode": 200
    },
    {
      "source": "/blog",
      "destination": "https://mind.com/blog",
      "statusCode": 301
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "support.imind.ru"
        }
      ],
      "destination": "https://mind.com/resources/help",
      "statusCode": 301
    },
    {
      "source": "/(.*)",
      "destination": "https://mind.com/promo/imind",
      "statusCode": 301
    }
  ]
}
